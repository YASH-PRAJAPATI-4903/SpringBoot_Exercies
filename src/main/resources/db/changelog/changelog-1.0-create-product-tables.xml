<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
       http://www.liquibase.org/xml/ns/dbchangelog
       https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="create-product-table" author="yash">
        <createTable tableName="product">
            <column name="id" type="LONG">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="productName" type="VARCHAR(255)"/>
            <column name="productBrand" type="VARCHAR(255)"/>
            <column name="availabe" type="BOOLEAN"/>
            <column name="count" type="LONG"/>
            <column name="packgeGram" type="DOUBLE"/>
            <column name="price" type="DOUBLE"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
