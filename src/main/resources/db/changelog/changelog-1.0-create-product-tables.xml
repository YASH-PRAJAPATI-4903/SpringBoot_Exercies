<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
       http://www.liquibase.org/xml/ns/dbchangelog
       https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="create-product-table-schema-product" author="yash">
        <createTable tableName="product" schemaName="product">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="product_name" type="VARCHAR(255)"/>
            <column name="product_brand" type="VARCHAR(255)"/>
            <column name="availabe" type="BOOLEAN"/>
            <column name="count" type="BIGINT"/>
            <column name="packge_gram" type="DOUBLE"/>
            <column name="price" type="DOUBLE"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
